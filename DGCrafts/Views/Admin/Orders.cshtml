@model IEnumerable<ShippingInfo>

@{   Layout = "~/Views/Shared/_AdminLayout.cshtml";}


<h3>Orders</h3>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Customer</th>
            <th>Crafts Ordered</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
        <tr>
            <td>@order.CustomerName</td>
         </tr>
        }

    </tbody>
</table>


