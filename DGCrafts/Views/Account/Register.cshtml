@model Customer
@{
    ViewData["Title"] = "Register";
}

<h3>Registration</h3>

<div class="text-center">
    
    <form>
        <div class="form-group col-form-label-sm">
            <label  asp-for="Email"></label>
            <input type="email" />
        </div>
        
        <div class="form-group">
            <label asp-for="FirstName"></label>
            <input type="text" />
        </div>
       
        <div class="form-group">
            <label asp-for="LastName"></label>
            <input type="text" />
        </div>
        
        <div class="form-group">
            <label asp-for="Password"></label>
            <input type="password" />
        </div>
       

        <input type="submit" class="btn btn-dark" value="Register!"/>

        <input type="submit" value="Cancel" class="btn btn-danger" asp-action="Index">
       


    </form>
</div>